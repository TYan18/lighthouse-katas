    "def repeatNumbers(data):\n",
    "    empstr = \"\"\n",
    "    for i in range(len(data) - 1):\n",
    "        empstr += (str(data[i][0]) * data[i][1]) + \", \"\n",
    "    empstr += (str(data[-1][0]) * data[-1][1])\n",
    "        \n",
    "    return empstr"
