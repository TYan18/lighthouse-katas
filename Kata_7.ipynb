    "def checkAir(samples, threshold):\n",
    "    dirty = 0\n",
    "    total = len(samples)\n",
    "    for sample in samples:\n",
    "        if sample == 'dirty':\n",
    "            dirty += 1\n",
    "    percent = dirty / total\n",
    "    if percent > threshold:\n",
    "        return \"Polluted\"\n",
    "    else:\n",
    "        return \"Clean\""
   ]
